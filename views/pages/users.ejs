<%- include('../partials/header') %>
<nav class="navbar navbar-expand-lg navbar-light fixed-top" id="mainNav">
</nav>
    <div class="container">
      <h1><span style="text-transform:uppercase;"><%= location %></span></h1>
      <h4 class="text-info"><%= title %> <span class="badge badge-info"><%= item.length %></span></h4>
      <nav class="navbar navbar-expand-lg navbar-light bg-light mb-4">
        <a class="navbar-brand" href="#">Dashboard</a>
        <button class="navbar-toggler" type="button" data-toggle="collapse" data-target="#navbarSupportedContent" aria-controls="navbarSupportedContent" aria-expanded="false" aria-label="Toggle navigation">
          <span class="navbar-toggler-icon"></span>
        </button>

        <div class="collapse navbar-collapse" id="navbarSupportedContent">
          <ul class="navbar-nav mr-auto">
            <li class="nav-item">
              <a class="nav-link" href="./singapore">Singapore</a>
            </li>
            <li class="nav-item">
              <a class="nav-link" href="./indonesia">Indonesia</a>
            </li>
          </ul>
          <a href="./export/<%= location %>" class="btn btn-success" >Export to Excel</a>
        </div>
      </nav>
      <table class="table table-bordered" style="font-size:11px;">
        <thead>
          <tr>
            <th scope="col">#</th>
            <th scope="col">Name</th>
            <th scope="col">Email</th>
            <th scope="col">Country</th>
            <th scope="col">First Day</th>
            <th scope="col">Second Day</th>
            <th scope="col">First Meeting</th>
            <th scope="col">Personal Meeting</th>
            <th scope="col">Contact Person</th>
            <th scope="col">Event Location</th>
            <th scope="col">Registered On</th>
          </tr>
        </thead>
        <tbody>
          
          <% item.forEach(function(attendee, index) { %>      
          <tr>
            <th scope="row"><%= index+1 %></th>
            <td><%= attendee.name %></td>
            <td><%= attendee.email %></td>
            <td><%= attendee.country %></td>
            <td><%= attendee.firstEvent %></td>
            <td><%= attendee.secondEvent %></td>
            <td><%= attendee.firstMeeting %></td>
            <td><%= attendee.personalMeeting %></td>
            <td><%= attendee.isContactPerson %></td>
            <td><%= attendee.eventCode %></td>            
            <td><%= attendee.updated %></td>
          </tr>
          <% }); %>
        </tbody>
      </table>
    </div>
    <!-- Bootstrap core JavaScript -->
    <script src="<%= host %>/js/jquery/jquery.min.js"></script>
    <script src="<%= host %>/js/bootstrap/js/bootstrap.bundle.min.js"></script>
</body>
</html>